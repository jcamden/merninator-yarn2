{
  // Yarn 2 business
  "search.exclude": {
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "eslint.nodePath": ".yarn/sdks",
  "prettier.prettierPath": ".yarn/sdks/prettier/index.js",
  "typescript.tsdk": "/media/jcamden/Storage/projects/code/current/merninator-yarn2/.yarn/sdks/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  // My preferences
  "editor.fontFamily": "JetBrains Mono",
  "editor.fontLigatures": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "files.autoSave": "onFocusChange",
  // Formatting
  "editor.formatOnSave": true,
  // not sure if this is working...
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "prettier.jsxSingleQuote": true,
  "prettier.packageManager": "yarn",
  "sort-imports.default-sort-style": "module",
  "sort-imports.languages": ["javascript", "typescript"],
  // candy
  "bracket-pair-colorizer-2.activeScopeCSS": [
    "borderStyle : solid",
    "borderWidth : 1px",
    "borderColor : {color}",
    "opacity: 0.5"
  ],
  "workbench.iconTheme": "material-icon-theme",
  "material-icon-theme.activeIconPack": "react",
  "material-icon-theme.languages.associations": {},
  "material-icon-theme.folders.associations": {
    "client": "App",
    "lib": "Tools",
    "components": "Components",
    "atoms": "React-components",
    "molecules": "Cluster",
    "organisms": "Debug",
    "ui": "Shared",
    "contexts": "Class",
    "context": "Class",
    "storybook": "Sass"
  },
  "material-icon-theme.files.associations": {}
}
